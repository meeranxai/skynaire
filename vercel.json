{
  "buildCommand": "echo 'Using static files from frontend/public'",
  "outputDirectory": "frontend/public",
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.html"
    }
  ]
}


